{
  "policies": [
    {
      "handler": {
        "export": "ApiKeyInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "allowUnauthenticatedRequests": false,
          "cacheTtlSeconds": 60
        }
      },
      "name": "api-key-inbound",
      "policyType": "api-key-inbound"
    },
    {
      "handler": {
        "export": "RateLimitInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "rateLimitBy": "function",
          "identifier": {
            "export": "rateLimit",
            "module": "$import(./modules/rate-limit)"
          }
        }
      },
      "name": "rate-limit-inbound",
      "policyType": "rate-limit-inbound"
    },
    {
      "handler": {
        "export": "SetHeadersInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "headers": [
            {
              "name": "fakeZuploApiKey",
              "value": "$env(BACKEND_SECRET)"
            }
          ]
        }
      },
      "name": "set-headers-inbound",
      "policyType": "set-headers-inbound"
    },
    {
      "handler": {
        "export": "default",
        "module": "$import(./modules/forwardOrganizationIds)",
        "options": {}
      },
      "name": "custom-code-inbound",
      "policyType": "custom-code-inbound"
    },
    {
      "handler": {
        "export": "SetQueryParamsInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "params": [
            {
              "name": "my-key",
              "value": "my-value"
            }
          ]
        }
      },
      "name": "set-query-params-inbound",
      "policyType": "set-query-params-inbound"
    }
  ]
}